    <hs:item>
      <hs:file_video>ad_drink_juice_sis.flv</hs:file_video>
      <hs:lang_en><![CDATA[عصير]]></hs:lang_en>
      <hs:lang_tl dir="rtl"><![CDATA[أختي تشرب عصير المانجو]]></hs:lang_tl>
      <hs:lang_trans><![CDATA[]]></hs:lang_trans>
